<style>
* {
  margin: 0;
  padding: 0;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}

html{
  overflow-x: hidden;
}

html:not([data-scroll='0']) body {
    padding-top: 0em;
}

html:not([data-scroll='0']) header {
    position: fixed;
    top: 0;
    background-color: rgba(40, 52, 63, 0.87);
    box-shadow: 0 0 0.7em rgba(0, 0, 0, 0.856);
}
  

body{
  background-attachment: fixed;
  font-family: 'Robot',sans-serif;
  background-repeat: no-repeat;
  background-image: url("/images/background.jpg");
  background-size: cover;
  font-size: 16px;
}

body::-webkit-scrollbar {
  width: 3px;
}

body::-webkit-scrollbar-track {
  -webkit-box-shadow: inset 0 0 6px white;
          box-shadow: inset 0 0 6px white;
}

body::-webkit-scrollbar-thumb {
  background-color: #334658;
}


header {
    width: 100%;
    height: 5rem;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-family: 'Varela Round',sans-serif;
    z-index: 5;
}

.logo #logoicon{
    height: 8%;
    width: 8%;
    margin: 1% 1% 1% 1%;
}

.logo #texticon{
    width: 20%;
}

.hamburger {
    display: none;
}

.line {
    width: 40px;
    height: 3px;
    background-color: #fff;
    margin-bottom: 5px;
    border-radius: 5px;
}



.navigation {
    transition: all .6s ease-in-out;
    margin-top: 1.5rem;
}

.navigation ul {
    display: flex;
}

.navigation ul li {
    list-style: none;
    margin-right: 1rem;
}

.navigation ul li a {
    text-decoration: none;
    color: #59f0f5;
    font-size: 1.6rem;
    line-height: 2.3rem;
    width: 100%;
    padding: 2rem;
    display: inline-block;
    transition: all .3s ease;
}

.navigation ul li a::after{
    content: '';
   display: block;
   width: 0;
   margin-top: 2px;
   height: 2px;
   background: #ae2fb3;
   -webkit-transition: width .3s;
   transition: width .3s;
 }

.navigation ul li a:hover{
    text-decoration: underline;
    color: rgb(116, 243, 243);
}

.navigation ul li a:hover::after{
    width: 100%;
}



 .navigation.active {
 left: 0;
}

/* .sticky {
    position: fixed;
    top: 0;
    width: 100%;
  } */


/*center animation*/

#center-items{
    display: flex;
    margin-top: 3%;
    justify-content: space-between;
    align-items: center;
}

#center-items svg{
    margin-top: 4%;
}

#inner-play{
    animation: move1 1.5s ease-out .3s alternate infinite;
  }
  
  #play-btn{
    animation: move1 1.5s ease-out .5s alternate infinite;
  }
  
  @keyframes move1{
    0%{
      transform: translate(-1%,0);
      opacity: .2;
    }
    100%{
      transform: translate(4%,0);
      opacity: 1;
    }
  
  }
  
  /* rotate animation center*/
  
  #rotator1{
    animation: rotate1 1.5s ease-in-out .3s forwards 1;
    transform-origin: center;
  }
  
  #rotator2{
    animation: rotate2 1.5s ease-in-out .3s forwards 1;
  }
  
  @keyframes rotate1{
    from{
      transform: rotateZ(0deg);
    }
    to{
      transform: rotateZ(360deg);
    }
  }
  
  @keyframes rotate2{
    from{
      transform: rotateZ(0deg);
    }
    to{
      transform: rotateZ(-360deg);
    }
  }

/*about*/

#about{
    margin-top: 20%;
    background-color: #2a9c9cbd;
    font-family: 'Varela Round',sans-serif;
}
  
#about .text{
    color: white;
    width: 50%;
    padding-bottom: 3rem;
    margin: 5rem auto 0 auto;
    line-height: 125%;
    font-size: 1.5rem;
    text-align: center;
}
  
.eastern{
    display: flex;
    justify-content: center;
}

/*Main - attraction*/
#main-attraction{
    padding: 0 0 3% 0;
    margin: 15vh 0 0 0;
    text-align: center;
  
  }
  
  #main-attraction h1{
    font-size: 4rem;
    color: white;
    margin-bottom: 10vh;
    font-family: 'Varela Round',sans-serif;
  }
  
  .guest{
    width: 100%;
    max-height: 50vh;
    display: flex;
    justify-content: space-around;
    margin: 5% 0;
  }

  .guestname{
    font-size: 2.5rem;
  }
  
  .description{
    width: 40%;
    color: #ffffff;
    font-size: 1.5rem;
    line-height: 125%;
    font-family: 'Varela Round',sans-serif;
  }
  
  
  
  #rito{
    width: 35%;
    height: 25%;
    
  }
  
  #riddles{
    width: 35%;
    height: 17%;
  }

/*Main Events*/

  .main-events{
    background-color: rgba(248, 240, 171, 0.836);
    padding-bottom: 2%;
  }
  
  .main-events h1{
    text-align: center;
    font-size: 4rem;
    color: rgb(0, 0, 0);
    padding-top: 10vh;  
    margin-bottom: 10vh;
    font-family: 'Varela Round',sans-serif;
  }
  
  .event{
    display: flex;
    justify-content: space-around;
    width: 80%;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 10%;
    padding: 5%;
    border-radius: 10px;
    font-family: 'Varela Round', sans-serif;
    transition: all .3s ease-out;
  }

  .eventname{
      font-size: 2.5rem;
  }
  
  .event:hover{
    transform: scale(1.05);
  }
  
  #battleofbands{
    -webkit-box-shadow: 0px 0px 29px -10px rgba(120,120,112,1);
    -moz-box-shadow: 0px 0px 29px -10px rgba(120,120,112,1);
    box-shadow: 0px 0px 29px -10px rgba(120,120,112,1);
    background-color: #faf09c;
  }
  
  #dance{
    -webkit-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
    -moz-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
     box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
    background-color: #ee475b;
  
  }
  
  #singing{
    -webkit-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
    -moz-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
     box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
    background-color: #ffffff;
  }
  
  #beatbox{
    -webkit-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
    -moz-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
     box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
     background-color: #bfdcf8;
  }
  
  #openmic{
    -webkit-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
    -moz-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
     box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
     background-color: rgb(248, 179, 52);
  }
  
  #mr-ms{
    -webkit-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
    -moz-box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
     box-shadow: 0px 0px 29px -9px rgba(0,0,0,1);
     background-color: rgb(54, 35, 97);
  }
  
  
  #battleofbands img{
    width: 30%;
    height: 25%;
  }
  #singing img{
    width: 35%;
    height: 40%;
  }
  
  #openmic img{
    width: 35%;
    height: 40%;
  }
  
  #mr-ms img{
      width: 45%;
      height: 50%;
    }
  
  .info{
    width: 40%;
    color: #000000;
    font-size: 1.5rem;
    line-height: 125%;
    text-align: center;
    font-family: 'Varela Round',sans-serif;
  }
  
  .info a{
    margin: 3%;
    padding: 1%;
    text-decoration: none;
    color:rgb(255, 255, 255);
    border-radius: 10px;
    background-color: rgb(132, 82, 248);
  }
  
  .info a:hover{
    color: rgb(0, 0, 0);
  }
  
  /* footer*/

  /*Footer*/
.footer{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    font-family: 'Varela Round',sans-serif;
    background-color: rgb(40, 52, 63);
}

.social a{
  margin: 0 4% 0 4%;
}
  
  .contact-us{
    display: flex;
    margin-top: 2%;
    flex-direction: row;
    justify-content: space-around;
  }
  
  .ourname{
    text-align: center;
    padding: 0;
    margin: 0;
  }

